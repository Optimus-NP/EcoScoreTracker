{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "SESSION_SECRET": "@session_secret"
  },
  "functions": {
    "app.py": {
      "maxDuration": 60
    }
  }
}